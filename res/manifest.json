{
  "manifest_version": 2,
  "name": "Tabs store",
  "description": "Tabs store",
  "version": "0.0.19",
  "icons": {
    "32": "icons/icon.svg",
    "48": "icons/icon.svg",
    "64": "icons/icon.svg"
  },
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "sidebar_action": {
    "default_title": "Stored Tabs",
