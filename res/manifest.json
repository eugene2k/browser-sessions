    "default_icon": "icons/icon.svg",
    "open_at_install": true
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "permissions": [
    "alarms",
    "tabs",
    "sessions",
    "storage",
    "bookmarks"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{e796cf79-057f-4d74-b03f-03344f8c837d}"
    }
  }
}