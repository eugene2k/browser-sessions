{
  "manifest_version": 2,
  "name": "browser-sessions",
  "description": "Browser Sessions Manager",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "browser-sessions"
  },
  "page_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "pageAction/index.html",
    "default_title": "browser-sessions"
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "permissions": [
    "tabs"
  ]
}