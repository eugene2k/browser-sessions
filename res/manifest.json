{
  "manifest_version": 2,
  "name": "browser-sessions",
  "description": "Browser Sessions Manager",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "chrome_settings_overrides": {
    "homepage": "homePage/index.html"
  },
  "chrome_url_overrides": {
    "newtab": "homePage/index.html"
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "permissions": [
    "tabs",
    "topSites"
  ]
}