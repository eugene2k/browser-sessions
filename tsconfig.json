{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "lib": [
      "es6",
      "DOM"
    ],
    "target": "ESNext",
    "declaration": false,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "module": "preserve",
  }
}